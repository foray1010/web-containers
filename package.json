{
  "name": "web-containers",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.9",
    "npm": ">=6.6"
  },
  "scripts": {
    "build:dev:watch:dirty": "cross-env NODE_ENV=development rollup -c -w",
    "build:dev": "shx rm -rf dist/ && cross-env NODE_ENV=development rollup -c",
    "build:prod": "shx rm -rf dist/ && cross-env NODE_ENV=production rollup -c",
    "ci": "run-p build:prod lint test typecheck",
    "predev": "npm run build:dev",
    "dev": "run-p --print-label --print-name build:dev:watch:dirty start:firefox",
    "eslint": "eslint --ext=js,mjs,ts,tsx --ignore-path=node_modules/@foray1010/eslint-config/eslintignore",
    "lint": "npm run eslint .",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettierrc/prettierignore",
    "start:firefox": "web-ext run --source-dir dist/",
    "test": "jest --config .jestrc.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/polyfill": "7.6.0",
    "escape-string-regexp": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "7.6.0",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@foray1010/eslint-config": "1.3.0",
    "@foray1010/import-sort-config": "1.1.0",
    "@foray1010/prettierrc": "1.1.0",
    "@types/babel__core": "7.1.3",
    "@types/eslint": "6.1.2",
    "@types/firefox-webext-browser": "67.0.2",
    "@types/jest": "24.0.18",
    "@types/prettier": "1.18.3",
    "@types/rollup-plugin-json": "3.0.2",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "cross-env": "6.0.3",
    "eslint": "6.5.1",
    "fast-glob": "3.1.0",
    "husky": "3.0.8",
    "import-sort-cli": "6.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.18",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.18.2",
    "rollup": "1.23.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "5.1.0",
    "rollup-plugin-terser": "5.1.2",
    "shx": "0.3.2",
    "svelte": "2.16.1",
    "typescript": "3.6.4",
    "web-ext": "3.2.0"
  }
}
