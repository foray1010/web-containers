{
  "name": "web-containers",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.9",
    "npm": ">=6.6"
  },
  "scripts": {
    "build:dev": "shx rm -rf dist/ && cross-env NODE_ENV=development rollup -c",
    "build:dev:watch:dirty": "cross-env NODE_ENV=development rollup -c -w",
    "build:prod": "shx rm -rf dist/ && cross-env NODE_ENV=production rollup -c",
    "ci": "run-p build:prod lint test typecheck",
    "predev": "npm run build:dev",
    "dev": "run-p --print-label --print-name build:dev:watch:dirty start:firefox",
    "lint": "eslint --ext html,js,ts .",
    "start:firefox": "web-ext run --source-dir dist/",
    "test": "jest --config .jestrc.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/polyfill": "7.6.0",
    "escape-string-regexp": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-typescript": "7.6.0",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/firefox-webext-browser": "67.0.2",
    "@types/jest": "24.0.18",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "cross-env": "5.2.1",
    "eslint": "6.3.0",
    "eslint-config-foray1010": "8.0.0",
    "fast-glob": "3.0.4",
    "husky": "3.0.5",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.17",
    "postcss-preset-env": "6.7.0",
    "prettier-eslint-cli": "5.0.0",
    "rollup": "1.20.3",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "5.1.0",
    "rollup-plugin-terser": "5.1.1",
    "shx": "0.3.2",
    "svelte": "2.16.1",
    "typescript": "3.6.2",
    "web-ext": "3.1.1"
  }
}
