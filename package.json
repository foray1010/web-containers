{
  "name": "web-containers",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.9",
    "npm": ">=6.6"
  },
  "scripts": {
    "build:dev": "shx rm -rf dist/ && cross-env NODE_ENV=development rollup -c",
    "build:dev:watch:dirty": "cross-env NODE_ENV=development rollup -c -w",
    "build:prod": "shx rm -rf dist/ && cross-env NODE_ENV=production rollup -c",
    "ci": "run-p lint test typecheck",
    "predev": "npm run build:dev",
    "dev": "run-p --print-label --print-name build:dev:watch:dirty start:firefox",
    "lint": "eslint '**/*.{js,ts}'",
    "start:firefox": "web-ext run --source-dir dist/",
    "test": "jest --config .jestrc.json",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "escape-string-regexp": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.3.3",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "7.3.3",
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/escape-string-regexp": "1.0.0",
    "@types/firefox-webext-browser": "65.0.0",
    "@types/jest": "24.0.6",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "cross-env": "5.2.0",
    "eslint": "5.14.0",
    "eslint-config-foray1010": "7.0.0",
    "husky": "1.3.1",
    "import-sort-cli": "5.2.0",
    "import-sort-parser-babylon": "5.2.0",
    "import-sort-parser-typescript": "5.0.0",
    "import-sort-style-module": "5.0.0",
    "jest": "24.1.0",
    "lint-staged": "8.1.4",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.14",
    "postcss-preset-env": "6.5.0",
    "prettier-eslint-cli": "4.7.1",
    "rollup": "1.2.1",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-copy": "0.2.3",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-svelte": "5.0.1",
    "rollup-plugin-terser": "4.0.4",
    "shx": "0.3.2",
    "svelte": "2.16.1",
    "typescript": "3.3.1",
    "web-ext": "2.9.3"
  }
}
